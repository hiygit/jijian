\n            } else {\r\n              this.$message.error(`失败`);\r\n            }\r\n          },\r\n          err => {\r\n            this.$message.error(`失败`);\r\n          }\r\n        );\r\n      }else{\r\n        \r\n        if(this.nextspan < 2){\r\n          this.step = this.nextspan\r\n          this.isShow = false\r\n        }else if(this.nextspan > 9){\r\n          this.step = this.nextspan==10?3:this.nextspan==11?4:''\r\n          this.isShow = false\r\n        }else{\r\n          this.step = 2\r\n          this.isShow =true\r\n          this.pages = this.nextspan-1\r\n        }\r\n\r\n        let that = this\r\n        let querstr = \"\";\r\n        let id = \"\";\r\n        that.textareaobj.textarealist.map(function(list){\r\n          if(that.textareaobj.disponsalWay == list.disponsalWay){\r\n            querstr = list.content\r\n            id = list.id\r\n          }\r\n        })\r\n        let requeststr = {\r\n          userId:JSON.parse(localStorage.getItem(\"userinfo\")).isd,\r\n          disponsalWay:that.textareaobj.disponsalWay,\r\n          content:querstr,\r\n          'dw':that.proposedDispositionList[that.nextspan].value,\r\n          id:id,\r\n          'cluesParameter.id': this.$route.query.id\r\n        }\r\n        requeststr = JSON.parse(this.$route.query.str)\r\n        requeststr.disponsalWay = that.disponsalWay+\"\"\r\n        requeststr.content = that.textareaobj.textarealist[that.pages - 1].content\r\n        requeststr.recordId = that.textareaobj.textarealist[that.pages - 1].id\r\n        requeststr.dw = that.proposedDispositionList[that.nextspan].value,\r\n        this.$ajax({\r\n          method: \"post\",\r\n          url: this.globals.requesturl + \"web/cluesparameter/firstSave\",\r\n          data: requeststr,\r\n          headers: { \"Content-Type\": \"application/x-www-form-urlencoded\" },\r\n          transformRequest: [\r\n            function(data) {\r\n              return qs.stringify(data);\r\n            }\r\n          ]\r\n        }).then(res => {\r\n          if (res.data.errorCode == \"-1\") {\r\n            this.$message.success(res.data.msg);\r\n            // this.$router.push({path:'/standBook'})\r\n            this.personnelpops = false\r\n          } else {\r\n            this.$message.error(res.data.msg);\r\n          }\r\n        })\r\n      }\r\n    }\r\n  }\r\n};\r\n",{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+NA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;;AAGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;;AAGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/views/updateprogress","sourcesContent":["<template>\r\n  <div class=\"xiansuotaizhang\">\r\n    <!-- 面包屑导航 -->\r\n    <div class=\"navcrumbed\">\r\n      <div class=\"navcrumbedimg\">\r\n        <router-link :to=\"{path: '/StandBook'}\"><img style=\"margin-top: 30px\" src=\"@/assets/navblack.png\" alt srcset /></router-link>\r\n        <!-- <img src=\"@/assets/navblack.png\" alt srcset /> -->\r\n        <span>当前位置：</span>\r\n      </div>\r\n      <div class=\"navcrumbedlist\">\r\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n          <el-breadcrumb-item :to=\"{ path: '/StandBook' }\">线索台账</el-breadcrumb-item>\r\n          <el-breadcrumb-item>更新进度</el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n      </div>\r\n    </div>\r\n    <div class=\"content home\">\r\n      <div class=\"StandBookEnter\">\r\n        <span class=\"border row1\"></span>\r\n        <span class=\"border row2\"></span>\r\n        <span class=\"border col1\"></span>\r\n        <span class=\"border col2\"></span>\r\n        <div class=\"StandBookEnterMain\" style=\"display: flex\">\r\n          <!-- 左侧 进程列表 -->\r\n          <div class=\"progressListLeftBox\">\r\n            <div class=\"progressListLeftTop\">\r\n              <div class=\"progressListbox\">\r\n                <img src=\"../../assets/schedule.png\" alt />\r\n                <span>进程列表</span>\r\n              </div>\r\n              <div class=\"topStepText\">\r\n                <p class=\"texto steptexto\" @click=\"leftget(0)\">\r\n                  <img src=\"../../assets/steponesel.png\" v-if=\"step == 0\" />\r\n                  <img src=\"../../assets/stepone.png\" v-else />\r\n                  <span :class=\"step == 0 ? 'stepselspan':''\">收件</span>\r\n                </p>\r\n                <p class=\"texto\" :class=\"step >= 1 ?'steptexto':''\" @click=\"leftget(1)\">\r\n                  <img src=\"../../assets/steptwosel.png\" v-if=\"step == 1\" />\r\n                  <img src=\"../../assets/steptwo.png\" v-else />\r\n                  <span :class=\"step == 1 ? 'stepselspan':''\">每周召开线索研判会</span>\r\n                </p>\r\n                <p class=\"texto\" :class=\"step >= 2 ?'steptexto':''\" @click=\"leftget(2)\">\r\n                  <img src=\"../../assets/stepthreesel.png\" v-if=\"step == 2\" />\r\n                  <img src=\"../../assets/stepthree.png\" v-else />\r\n                  <span :class=\"step == 2 ? 'stepselspan':''\">问题线索处置</span>\r\n                </p>\r\n                <p class=\"texto\" @click=\"leftget(3)\">\r\n                  <img src=\"../../assets/stepfoursel.png\" v-if=\"step == 3\" />\r\n                  <img src=\"../../assets/stepfour.png\" v-else />\r\n                  <span :class=\"step == 3 ? 'stepselspan':''\">处置结果落实</span>\r\n                </p>\r\n                <p class=\"texto lastTexto\" @click=\"leftget(4)\">\r\n                  <img src=\"../../assets/stepfivesel.png\" v-if=\"step == 4\" />\r\n                  <img src=\"../../assets/stepfive.png\" v-else />\r\n                  <span :class=\"step == 4 ? 'stepselspan':''\">归档</span>\r\n                </p>\r\n              </div>\r\n            </div>\r\n            <!-- 上传附件记录 -->\r\n            <div class=\"progressListLeftbottom\">\r\n              <div class=\"progressListbox\">\r\n                <img src=\"../../assets/accessory.png\" alt />\r\n                <span>上传附件记录</span>\r\n              </div>\r\n              <div class=\"accessoryBottom\">\r\n                <div\r\n                  class=\"accessoryBottom-text\"\r\n                  v-for=\"(item,index) in fileRecordList\"\r\n                  :key=\"index\"\r\n                >\r\n                  <p>\r\n                    <span>{{item.number}}{{(index+1 + '、')}}</span>\r\n                    {{item.file}}\r\n                  </p>\r\n                  <p style=\"text-align: right;color: #CBD9E8\">{{item.time}}</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 右侧 详细进度 -->\r\n          <div class=\"progressListRightBox\">\r\n            <div class=\"detailProgressTopBox\">\r\n              <p>\r\n                <img src=\"../../assets/amend.png\" alt />\r\n                <span>详细进度</span>\r\n              </p>\r\n              <!-- <button class=\"bottomBtnStyle\" @click=\"saverecord\">上传附件</button> -->\r\n              <div class=\"filesbtns\">\r\n                <el-button size=\"small\" type=\"primary\">上传附件</el-button>\r\n                <input\r\n                  type=\"file\"\r\n                  value=\"上传附件\"\r\n                  class=\"filesbtns-input\"\r\n                  multiple\r\n                  accept\r\n                  @click=\"headgetfile($event)\"\r\n                  title=\" \"\r\n                />\r\n              </div>\r\n              <!-- <div class=\"\">\r\n\r\n              </div>-->\r\n            </div>\r\n            <!-- 详细进度下的tab切换 -->\r\n            <div class=\"detailTab\">\r\n              <!-- 切换按钮 -->\r\n              <div class=\"tabButton\" v-show=\"isShow\">\r\n                <div class=\"tabButtonOne\" @click=\"get(1)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img v-if=\"pages==1\" src=\"../../assets/img.png\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 1 ? 'news':''\">谈话</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(2)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==2\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 2 ? 'news':''\">函询</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(3)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==3\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 3 ? 'news':''\">谈话+函询</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(4)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==4\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 4 ? 'news':''\">初核</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(5)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==5\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 5 ? 'news':''\">立案</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(6)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==6\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 6 ? 'news':''\">暂存</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(7)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==7\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 7 ? 'news':''\">了结</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(8)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==8\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 8 ? 'news':''\">转办</span>\r\n                </div>\r\n              </div>\r\n\r\n              <!-- 切换内容 -->\r\n              <div class=\"contentTabBox\">\r\n                <textarea v-model=\"content\" class=\"textareaContent\" cols=\"30\" rows=\"10\" \r\n                  placeholder=\"请输入\" :disabled=\"isdisable\">\r\n                </textarea>\r\n                <!-- <textarea v-model=\"textareaobj.textarealist[pages-1].content\" class=\"textareaContent\" cols=\"30\" rows=\"10\" placeholder=\"请输入\"\r\n                  :disabled=\"textareaobj.textarealist[pages-1].disponsalWay==textareaobj.disponsalWay\" >\r\n                </textarea> -->\r\n                \r\n                <!-- 谈话 -->\r\n                <!-- <div class=\"contentOneBox\" v-if=\"pages==1\"> \r\n                  <div class=\"talkbox\" v-for=\"(talkingList,index) in talkingLists\" :key=\"index\">\r\n                    <p>1. <input type=\"text\" v-model=\"talkingList.year\"> 年 <input type=\"text\" v-model=\"talkingList.month\"> 月 <input type=\"text\" v-model=\"talkingList.day\"> 日，驻市民政局纪检监察组向 <input type=\"text\" v-model=\"talkingList.name\"> 下达了函询通知书。</p>\r\n                    <p>附：做出说明的函</p>\r\n                  </div>\r\n                </div>-->\r\n              </div>\r\n            </div>\r\n\r\n            <!-- 按钮 -->\r\n            <div class=\"bottomButton\">\r\n              <button class=\"bottomBtnStyle\" @click=\"personnelpops = true\">下一步</button>\r\n              <button class=\"bottomBtnStyle\" @click=\"saverecord\">保存</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ========================================================================= -->\r\n    <!-- 下一步弹框 -->\r\n    <transition name=\"personnelframe\">\r\n      <div class=\"personnelframe\" v-if=\"personnelpops\">\r\n        <div class=\"personnelframe-close\"></div>\r\n        <div class=\"personnelframe-container\">\r\n          <div class=\"personnelframe-container-box\">\r\n            <div class=\"personnelframe-title\">\r\n              <img src=\"@/assets/courseIcon.png\" alt srcset />\r\n              <h3>进程更新</h3>\r\n            </div>\r\n            <div class=\"spanbox\">\r\n              <span v-for=\"(item,index) in proposedDispositionList\" :key=\"index\" @click=\"nextspan=index\" :class=\"nextspan == index ? 'lastspan':''\">{{item.label}}</span>\r\n              <!-- <span @click=\"nextspan=0\" :class=\"nextspan == 0 ? 'lastspan':''\">收件</span> -->\r\n            </div>\r\n          </div>\r\n          <div class=\"personnelframe-btns\">\r\n            <el-button type=\"primary\" @click=\"personnelpops=false\">取消</el-button>\r\n            <el-button type=\"warning\" @click=\"submitchangeunit\">确定</el-button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n    <!-- ========================================================================= -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport qs from \"qs\";\r\nexport default {\r\n  name: \"StandBookEnter\",\r\n  // props: {},\r\n  data() {\r\n    return {\r\n      filesarr: [],//上传文件name数组\r\n      fileList: [],\r\n      isdisable:false,//文本域内容是否可编辑\r\n      id: [],\r\n      isShow: false,//问题线索处置的子集 step=2时\r\n      personnelpops: false, //弹框显示切换\r\n      bodydisponsalWay: \"\", //处置方式\r\n      fileRecordList: [], //处置方式记录附件列表\r\n      content: [],\r\n      contenarr: [], //文本域内容\r\n      proposedDispositionList: [],//字典查询 问题线索处置的子集的\r\n      pages: 1,\r\n      step: null, //进度步骤\r\n      nextspan: 0,//下一步弹窗中span下标\r\n      textareaobj: {\r\n        disponsalWay: \"\",\r\n        content: \"\",\r\n        textarealist: [\r\n          { content: \"\", id: \"\", disponsalWay: 2 },\r\n          { content: \"\", id: \"\", disponsalWay: 3 },\r\n          { content: \"\", id: \"\", disponsalWay: 4 },\r\n          { content: \"\", id: \"\", disponsalWay: 5 },\r\n          { content: \"\", id: \"\", disponsalWay: 6 },\r\n          { content: \"\", id: \"\", disponsalWay: 7 },\r\n          { content: \"\", id: \"\", disponsalWay: 8 },\r\n          { content: \"\", id: \"\", disponsalWay: 9 }\r\n        ]\r\n      }\r\n    };\r\n  },\r\n  computed: {},\r\n  methods: {},\r\n  components: {},\r\n  created() {\r\n    document.title = \"线索台账修改进度\";\r\n\r\n      let that = this\r\n      //处置意见\r\n      this.$ajax({\r\n      method: \"POST\",\r\n      url: this.globals.requesturl + \"web/dict\",\r\n      data: { type: \"disponsal_way\" },\r\n      headers: { \"Content-Type\": \"application/x-www-form-urlencoded\" },\r\n      transformRequest: [\r\n        function(data) {\r\n          return qs.stringify(data);\r\n        }\r\n      ]\r\n    }).then(res=>{\r\n      if (res.data.success && res.data.errorCode == \"-1\") {\r\n        this.proposedDispositionList = res.data.body.dictValueList\r\n        // console.log(this.proposedDispositionList)\r\n      }else{\r\n        this.$message.error(res.data.msg);\r\n      }\r\n    })\r\n  },\r\n  mounted() {\r\n    let id = this.$route.query.id;\r\n    this.init();\r\n  },\r\n  watch: {},\r\n  methods: {\r\n    // 个人中心Tab切换\r\n    get(e) {\r\n      // console.log(e);\r\n      this.pages = e;\r\n    },\r\n    leftget(dex){\r\n      let that = this\r\n      let objs = {\r\n        id: this.$route.query.id,\r\n        userId: JSON.parse(localStorage.getItem(\"userinfo\")).isd\r\n      };\r\n      let acontent = {}\r\n      let totalList = [];\r\n      let contents = []; //文本域内容显示\r\n      let idid = [];\r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: that.globals.requesturl + \"web/cluesparameter/record\",\r\n        data: objs,\r\n        headers: { \"Content-Type\": \"application/x-www-form-urlencoded\" },\r\n        transformRequest: [\r\n          function(data) {\r\n            return qs.stringify(data);\r\n          }\r\n        ]\r\n      }).then(res=>{\r\n        if(res.data.success && res.data.errorCode == \"-1\"){\r\n          let arrlist = res.data.body.recordList\r\n          arrlist.map(function(list) {\r\n            acontent = list\r\n            // console.log(acontent)\r\n            // that.disponsalWay = list.disponsalWay;\r\n            contents.push(list.content);\r\n            idid.push(list.id);\r\n            totalList.push(list);\r\n            if(res.data.body.disponsalWay == list.disponsalWay){\r\n              // that.content = contents.join(\"\\n\")\r\n              that.isdisable = false\r\n            }\r\n          });\r\n\r\n          if(this.step == this.bodydisponsalWay){\r\n            this.isdisable = false\r\n            that.content = contents.join(\"\\n\")\r\n            that.id = idid[that.pages]\r\n            console.log(that.id)\r\n          }else{\r\n            that.content = \"\"\r\n            this.isdisable = true\r\n          }\r\n        }else{\r\n          this.$message.error(res.data.msg)