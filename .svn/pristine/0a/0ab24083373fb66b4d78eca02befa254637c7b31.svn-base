KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,OArBA;AAsBA,KA7IA;AA8IA;AACA,IAAA,WA/IA,uBA+IA,KA/IA,EA+IA;AACA,aAAA,KAAA,GAAA,CAAA,GAAA,CAAA,GAAA,KAAA,GAAA,CAAA,GAAA,OAAA,KAAA,GAAA,CAAA,CAAA;AACA,KAjJA;AAkJA;AACA,IAAA,IAnJA,gBAmJA,IAnJA,EAmJA;AAAA;;AACA,UAAA,IAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,MADA;AAEA,QAAA,GAAA,EAAA,IAAA,CAAA,OAAA,CAAA,UAAA,GAAA,kBAFA;AAGA,QAAA,IAAA,EAAA,IAHA;AAIA,QAAA,OAAA,EAAA;AAAA,0BAAA;AAAA,SAJA;AAKA,QAAA,gBAAA,EAAA,CACA,UAAA,IAAA,EAAA;AACA,iBAAA,EAAA,CAAA,SAAA,CAAA,IAAA,CAAA;AACA,SAHA;AALA,OAAA,EAUA,IAVA,CAUA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,OAAA,IAAA,GAAA,CAAA,IAAA,CAAA,SAAA,IAAA,CAAA,CAAA,EAAA;AACA,cAAA,OAAA,GAAA,EAAA;AACA,UAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,WAAA,CAAA,GAAA,CAAA,UAAA,IAAA,EAAA;AACA,gBAAA,IAAA,GAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,YAAA,IAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,YAAA,IAAA,CAAA,MAAA,GAAA,IAAA,CAAA,MAAA,IAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,KAAA;AACA,YAAA,IAAA,CAAA,EAAA,GAAA,IAAA,CAAA,EAAA;AACA,YAAA,OAAA,CAAA,IAAA,CAAA,IAAA;AACA,WAPA;AAQA,UAAA,IAAA,CAAA,SAAA,GAAA,OAAA;AACA,SAXA,MAWA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,OAzBA;AA0BA,KA/KA;AAgLA;AACA,IAAA,SAjLA,2BAiLA;AAAA,UAAA,GAAA,QAAA,GAAA;AAAA,UAAA,MAAA,QAAA,MAAA;AAAA,UAAA,QAAA,QAAA,QAAA;AAAA,UAAA,WAAA,QAAA,WAAA;;AACA;AACA,UAAA,QAAA,GAAA,CAAA,IAAA,CAAA,EAAA;AACA,eAAA;AACA,8BAAA,SADA;AAEA,wBAAA,QAFA;AAGA,UAAA,KAAA,EAAA,MAHA;AAIA,yBAAA,QAJA;AAKA,UAAA,OAAA,EAAA,OALA;AAMA,UAAA,MAAA,EAAA;AANA,SAAA;AAQA,OATA,MASA;AACA,eAAA;AACA,8BAAA,SADA;AAEA,wBAAA,QAFA;AAGA,UAAA,KAAA,EAAA,MAHA;AAIA,yBAAA,QAJA;AAKA,UAAA,OAAA,EAAA,OALA;AAMA,UAAA,MAAA,EAAA;AANA,SAAA;AAQA;AACA,KAtMA;AAuMA,IAAA,eAvMA,kCAuMA;AAAA,UAAA,GAAA,SAAA,GAAA;AAAA,UAAA,QAAA,SAAA,QAAA;AACA,aAAA;AACA,4BAAA,SADA;AAEA,sBAAA,QAFA;AAGA,QAAA,KAAA,EAAA,MAHA;AAIA,uBAAA,QAJA;AAKA,qBAAA;AALA,OAAA;AAOA;AA/MA,GAtDA;AAuQA,EAAA,KAAA,EAAA;AAvQA,CAAA","sourcesContent":["<template>\r\n  <div class=\"KeywordSet\">\r\n    <div class='BtnGroup'>\r\n        <div class=\"topBtnGroup\">\r\n            <button class=\"subBtn addbtn\" @click=\"addbtn\"><img src=\"../../assets/add.png\"/><span>添加</span></button>    \r\n            <!-- <button class=\"subBtn addbtn\"><img src=\"../../assets/amend.png\"/><span>修改</span></button>    \r\n            <button class=\"subBtn addbtn\"><img src=\"../../assets/del.png\"/><span>删除</span></button>     -->\r\n            <button class=\"subBtn addbtn\" @click=\"refresh\"><img src=\"../../assets/refresh.png\"/><span>刷新</span></button>    \r\n        </div>         \r\n    </div>\r\n    <el-table :data=\"tableData\" v-loading=\"loading\" border :height=\"tableHeight\" stripe style=\"background-color:transparent;width:100%;box-sizing:border-box; \" :cell-style=\"cellStyle\" :header-cell-style=\"headerCellStyle\">\r\n      <el-table-column label=\"序号\" prop=\"date\" width=\"150\" type=\"index\" :index=\"indexMethod\"></el-table-column>\r\n      <el-table-column label=\"关键词\" prop=\"status\">\r\n        <template scope=\"scope\">\r\n          <el-input readonly style=\"display:inline-block;width:90%;margin:0 auto;\" size=\"small\" v-model=\"scope.row.name\"/>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"排序\" prop=\"sort\">\r\n        <template scope=\"scope\">\r\n          <el-input readonly style=\"display:inline-block;width:90%;margin:0 auto;\" size=\"small\" v-model=\"scope.row.sort\"/>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"可见\" prop=\"isShow\">\r\n        <template scope=\"scope\">\r\n          <el-input readonly style=\"display:inline-block;width:90%;margin:0 auto;\" size=\"small\" v-model=\"scope.row.isShow\"/>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"450px\">\r\n        <template scope=\"scope\">\r\n          <div class=\"topBtnGroupTable\">\r\n            <button class=\"subBtnT addbtn\" @click=\"lookitem(scope.$index, scope.row)\">\r\n              <img src=\"../../assets/watch.png\" />\r\n              <span>查看</span>\r\n            </button>\r\n            <button class=\"subBtnT addbtn\" @click=\"updataitem(scope.$index, scope.row)\">\r\n              <img src=\"../../assets/amend.png\" />\r\n              <span>修改</span>\r\n            </button>\r\n            <button class=\"subBtnT addbtn\" @click=\"romoveitem(scope.$index, scope.row)\">\r\n              <img src=\"../../assets/amend.png\" />\r\n              <span>删除</span>\r\n            </button>\r\n          </div>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 删除弹框 -->\r\n    <transition name=\"personnelframe\">\r\n      <div class=\"delclose\" v-if=\"centerDialogVisible\">\r\n        <div class=\"personnelframe-close\"></div>\r\n        <div class=\"delclosebox\">\r\n          <h3>提示</h3>\r\n          <div class=\"deltit\">是否确认删除？</div>\r\n          <span slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"centerDialogVisible = false\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"subremove\">确 定</el-button>\r\n          </span>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n\r\n    <!-- 调动弹框 -->\r\n    <transition name=\"personnelframe\">\r\n      <div class=\"personnelframe\" v-if=\"personnelpops\">\r\n        <div class=\"personnelframe-close\"></div>\r\n        <div class=\"personnelframe-container\">\r\n          <div class=\"personnelframe-container-box\">\r\n            <div class=\"personnelframe-title\">\r\n              <!-- <img src=\"@/assets/diaodongxinxi.png\" alt srcset /> -->\r\n              <h3>关键字设置表单</h3>\r\n            </div>\r\n            <div class=\"personnelframe-pricker\">\r\n              <div class=\"personnelframe-pricker-box\">\r\n                <span class=\"personnelframe-pricker-box-span\">关键词：</span>\r\n                <input type=\"text\" value style=\"width: 220px\" v-model=\"updata.name\"/>\r\n              </div>\r\n              <div class=\"personnelframe-pricker-box\">\r\n                <span class=\"personnelframe-pricker-box-span\">排序：</span>\r\n                <input type=\"text\" value style=\"width: 220px\" v-model=\"updata.sort\"/>\r\n              </div>\r\n            </div>\r\n            <div class=\"personnelframe-pricker\">\r\n              <div class=\"personnelframe-pricker-box\">\r\n                <span class=\"personnelframe-pricker-box-span\">是否可见：</span>\r\n                <el-select v-model=\"updata.isShow\" placeholder=\"请选择\">\r\n                  <el-option style=\"font-size: 14px\" v-for=\"item in isif\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\"></el-option>\r\n                </el-select>\r\n              </div>\r\n              <div class=\"personnelframe-pricker-box\">\r\n                <span class=\"personnelframe-pricker-box-span\">备注：</span>\r\n                <textarea cols=\"30\" v-model=\"updata.remarks\" rows=\"10\" style=\"resize: none;padding: 5px 10px;box-sizing: border-box;width: 215px;height: 115px;border: 1px solid #BCD9E8;outline: none;background: none\"></textarea>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"personnelframe-btns\">\r\n            <el-button @click=\"quit\">取消</el-button>\r\n            <el-button type=\"primary\" @click=\"savebtnyes\" v-if=\"disable\">确定</el-button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport qs from 'qs'\r\nexport default {\r\n  name: \"KeywordSet\",\r\n  props: [\"tableHeight\"],\r\n  data() {\r\n    return {\r\n      loading:false,\r\n      disable: true,\r\n      centerDialogVisible:false,//是否确认删除\r\n      personnelpops: false, //弹框\r\n      value: \"\",\r\n      updata: {\r\n        name:\"\",\r\n        isShow:\"\",\r\n        sort:\"\",\r\n        id:\"\",\r\n        remarks:\"\"\r\n      },\r\n      ifadd:false,\r\n      tableData: [],\r\n      isif:[],//下拉选择\r\n      delval:\"\",//查看、删除下标\r\n      page:0,//第几页\r\n      pagesize:20,//每页显示条数\r\n    };\r\n  },\r\n  created() {\r\n    document.title = \"关键词设置\"\r\n    let that = this\r\n    this.$ajax({\r\n      method: \"POST\",\r\n      url: this.globals.requesturl + 'web/dict',\r\n      data: {type:'yes_no'},\r\n      headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n      transformRequest: [function (data) { \r\n        return qs.stringify(data)\r\n      }]\r\n    }).then(res=>{\r\n      if(res.data.success && res.data.errorCode == '-1'){\r\n        res.data.body.dictValueList.map(function(list){\r\n          that.isif.push({value:list.value,label:list.label})\r\n        })\r\n      }else{\r\n        this.$message.error(res.data.msg); \r\n      }\r\n    })\r\n  },\r\n  mounted() {\r\n    let objs = {\r\n      userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n      pageNo:this.page,\r\n      pageSize:this.pagesize,\r\n    } \r\n    this.init(objs);\r\n  },\r\n  methods: {\r\n    //刷新\r\n    refresh(){\r\n      let that = this\r\n      that.loading  =true\r\n      setTimeout(function(){\r\n        that.loading  =false\r\n      },1500)\r\n      let objs = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n        pageNo:this.page,\r\n        pageSize:this.pagesize,\r\n      } \r\n      this.init(objs);\r\n    },\r\n    //添加按钮\r\n    addbtn(){\r\n      this.ifadd = true\r\n      this.personnelpops = true\r\n    },\r\n    // 删除\r\n    romoveitem(row){\r\n      this.delval = row\r\n      this.centerDialogVisible = true\r\n    },\r\n    subremove(){\r\n      let that = this,id = that.tableData[that.delval].id;\r\n      let objs = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n        id:id,\r\n      } \r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: that.globals.requesturl + 'web/keyword/delete',\r\n        data: objs,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.success && res.data.errorCode == -1){\r\n          this.$message.success(res.data.msg); \r\n          that.tableData.splice(that.delval,1)\r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n        that.centerDialogVisible = false\r\n      })\r\n    },\r\n    //取消\r\n    quit(){\r\n      this.ifadd=false\r\n      this.readonly=false\r\n      this.personnelpops = false\r\n      this.updata = {\r\n        name:\"\",\r\n        isShow:\"\",\r\n        sort:\"\",\r\n        id:\"\",\r\n        remarks:\"\"\r\n      }\r\n    },\r\n    //保存\r\n    savebtnyes(){\r\n      let that = this\r\n      let objs = that.updata\r\n      objs.userId = JSON.parse(localStorage.getItem('userinfo')).isd\r\n      if(objs.name == \"\"){\r\n        this.$message.error(\"请填写关键字\"); \r\n      }else if(objs.isShow==\"\"){\r\n        this.$message.error(\"请选择是否可见\"); \r\n      }else if(objs.sort==\"\"){\r\n        this.$message.error(\"请填写排序\"); \r\n      }else{\r\n        this.$ajax({\r\n          method: \"POST\",\r\n          url: this.globals.requesturl + 'web/keyword/save',\r\n          data: objs,\r\n          headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n          transformRequest: [function (data) { \r\n            return qs.stringify(data)\r\n          }]\r\n        }).then(res=>{\r\n          if(res.data.success && res.data.errorCode == '-1'){\r\n            that.personnelpops = false\r\n            if(that.ifadd){\r\n              that.tableData.push(objs)\r\n            }else{\r\n              // this.tableData[that.delval] = objs\r\n              this.$set(this.tableData,that.delval,objs)\r\n              this.updata = {\r\n                name:\"\",\r\n                isShow:\"\",\r\n                sort:\"\",\r\n                id:\"\",\r\n                remarks:\"\"\r\n              }\r\n            }\r\n            this.$message.success(res.data.msg); \r\n            this.ifadd=false\r\n          }else{\r\n            this.$message.error(res.data.msg); \r\n          }\r\n        })\r\n      }\r\n    },\r\n    //查看\r\n    lookitem(row){\r\n      this.disable=false\r\n      this.updataitem(row)\r\n    },\r\n    //修改\r\n    updataitem(row){\r\n      let that = this\r\n      this.delval = row\r\n      let objs = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n        id:that.tableData[row].id\r\n      } \r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: this.globals.requesturl + 'web/keyword/form',\r\n        data: objs,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.success && res.data.errorCode == '-1'){\r\n          that.personnelpops = true\r\n          let objs = {};\r\n          objs.name = res.data.body.name\r\n          objs.sort = res.data.body.sort\r\n          objs.isShow = res.data.body.isShow\r\n          objs.id = res.data.body.id\r\n          objs.remarks = res.data.body.remarks\r\n          that.updata = objs\r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n      })\r\n    },\r\n    //下标处理\r\n    indexMethod(index){\r\n      return index+1>9?index+1:'0'+(index+1)\r\n    },\r\n    // 初始化\r\n    init(objs) {\r\n      let that = this;\r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: that.globals.requesturl + \"web/keyword/list\",\r\n        data: objs,\r\n        headers: { \"Content-Type\": \"application/x-www-form-urlencoded\" },\r\n        transformRequest: [\r\n          function(data) {\r\n            return qs.stringify(data);\r\n          }\r\n        ]\r\n      }).then(res => {\r\n        if (res.data.success && res.data.errorCode == -1) {\r\n          let listarr = [];\r\n          res.data.body.keyWordList.map(function(list){\r\n            let objs = {};\r\n            objs.name = list.name\r\n            objs.sort = list.sort\r\n            objs.isShow = list.isShow == that.isif[0].value?that.isif[0].label:that.isif[1].label\r\n            objs.id = list.id\r\n            listarr.push(objs)\r\n          })\r\n          that.tableData = listarr;\r\n        } else {\r\n          this.$message.error(res.data.msg);\r\n        }\r\n      });\r\n    },\r\n    // 表格样式的处理\r\n    cellStyle({ row, column, rowIndex, columnIndex }) {\r\n      //基数偶数延时不同\r\n      if (rowIndex % 2 != 0) {\r\n        return {\r\n          \"background-color\": \"#2162A8\",\r\n          \"text-align\": \"center\",\r\n          color: \"#fff\",\r\n          \"font-weight\": \"normal\",\r\n          padding: \"5px 0\",\r\n          height: \"60px\"\r\n        };\r\n      } else {\r\n        return {\r\n          \"background-color\": \"#02468F\",\r\n          \"text-align\": \"center\",\r\n          color: \"#fff\",\r\n          \"font-weight\": \"normal\",\r\n          padding: \"5px 0\",\r\n          height: \"60px\"\r\n        };\r\n      }\r\n    },\r\n    headerCellStyle({ row, rowIndex }) {\r\n      return {\r\n        \"background-color\": \"#027DBC\",\r\n        \"text-align\": \"center\",\r\n        color: \"#fff\",\r\n        \"font-weight\": \"normal\",\r\n        \"font-size\": \"16px\"\r\n      };\r\n    }\r\n  },\r\n  watch: {}\r\n};\r\n</script>\r\n\r\n<style  lang=\"scss\">\r\n.KeywordSet {\r\n  .topBtnGroupTable {\r\n    display: flex;\r\n    width: 80%;\r\n    margin: 0 auto;\r\n    justify-content: space-around;\r\n\r\n    .subBtnT {\r\n      overflow: hidden;\r\n      background: transparent;\r\n      border-radius: 3px;\r\n      width: 60px;\r\n      height: 30px;\r\n      line-height: 30px;\r\n      border