n qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.errorCode == \"-1\"){\r\n          let lists = [];\r\n          if(res.data.body.personnelTransferRecords.length != 0){\r\n            res.data.body.personnelTransferRecords.map(function(list,index){\r\n              lists.push({\r\n                times:list.createDate,\r\n                oldunit:list.office.name,\r\n                oldposition:list.originalPosition,\r\n                newunit:list.newOffice.name,\r\n                newposition:list.newOriginalPosition,\r\n                handles:list.createBy.name,\r\n                num:index+1 > 9?index+1:'0'+(index+1) ,\r\n                name: list.personnel.name,\r\n                sex:list.personnel.sex==0?'男':'女',\r\n                age:list.personnel.age,\r\n                id:list.personnel.id,\r\n                input:\"\",\r\n              })\r\n            })\r\n          }\r\n          that.tableData = lists\r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n      })\r\n    },\r\n    // 查询单位\r\n    initunits(){\r\n      let that = this; \r\n      let objs = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n      } \r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: that.globals.requesturl + 'web/office/list',\r\n        data: objs,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.errorCode == \"-1\"){\r\n          let lists = [];\r\n          res.data.body.officeList.map(function(list,index){\r\n            lists.push({\r\n              label:list.name,\r\n              value:list.id,\r\n              dex:index,\r\n              children: list.hasChildren==false?\"\":[{}],\r\n              ishasChildren:list.hasChildren==false?false:true\r\n            })\r\n          })\r\n          that.unitslist = lists\r\n          that.datas = lists\r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n      })\r\n    },\r\n    //级联事件\r\n    unitChanges(data) {\r\n      let that = this;\r\n      let objs = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n        parentId:data[0]\r\n      } \r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: that.globals.requesturl + 'web/office/getChildren',\r\n        data: objs,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.errorCode == \"-1\"){\r\n          let lists = [];\r\n          res.data.body.officeList.map(function(list,index){\r\n            lists.push({\r\n              label:list.name,\r\n              value:list.id,\r\n              dex:index,\r\n              children: list.hasChildren==false?\"\":[{}],\r\n              ishasChildren:list.hasChildren==false?false:true\r\n            })\r\n          })\r\n          let nums = 0;\r\n          that.unitslist.map(function(list,index){\r\n            if(list.value == data){\r\n              nums = index\r\n            }\r\n          })\r\n          that.unitslist[nums].children = lists\r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n      })\r\n    },\r\n    //确定调动\r\n    submitchangeunit(){\r\n      let that = this;\r\n      let querststr = {\r\n        personnelId:that.peoplemsg.id,\r\n        newOfficeId:that.unitvalue[that.unitvalue.length-1],\r\n        newOriginalPosition:that.peoplemsg.input,\r\n        userId:JSON.parse(localStorage.getItem('userinfo')).isd,\r\n      };\r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: that.globals.requesturl + 'web/personneltransfer/transfer',\r\n        data: querststr,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.success || res.data.errorCode == -1){\r\n          that.personnelpops = false\r\n          this.$message.success('调动成功'); \r\n        }else{\r\n          this.$message.error('调动失败'); \r\n        }\r\n      })\r\n    },\r\n    //搜素提交按钮\r\n    onSubmit(){\r\n      let searchstr = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n        name:this.formInline.region\r\n      }\r\n      this.init(searchstr)\r\n      this.treelist = false\r\n      this.searchlist = true\r\n    },\r\n    //树状图选择\r\n    handleNodeClick(data) {\r\n      let that = this; \r\n      if(data.ishasChildren){\r\n        let objs = {\r\n          userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n          parentId:data.value\r\n        } \r\n        this.$ajax({\r\n          method: \"POST\",\r\n          url: that.globals.requesturl + 'web/office/getChildren',\r\n          data: objs,\r\n          headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n          transformRequest: [function (data) { \r\n            return qs.stringify(data)\r\n          }]\r\n        }).then(res=>{\r\n          if(res.data.errorCode == \"-1\"){\r\n            let lists = [];\r\n            res.data.body.officeList.map(function(list,index){\r\n              lists.push({\r\n                label:list.name,\r\n                value:list.id,\r\n                dex:index,\r\n                children: list.hasChildren==false?\"\":[{}],\r\n                ishasChildren:list.hasChildren==false?false:true\r\n              })\r\n            })\r\n            that.datas[data.dex].children = lists\r\n          }else{\r\n            this.$message.error(res.data.msg); \r\n          }\r\n        })\r\n      }else{\r\n        that.treelist = false\r\n        that.searchlist = true\r\n        let objs = {\r\n          userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n          officeId:data.value\r\n        } \r\n        this.$ajax({\r\n          method: \"POST\",\r\n          url: that.globals.requesturl + 'web/personneltransfer/recordList',\r\n          data: objs,\r\n          headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n          transformRequest: [function (data) { \r\n            return qs.stringify(data)\r\n          }]\r\n        }).then(res=>{\r\n          if(res.data.errorCode == \"-1\"){\r\n            let lists = [];\r\n            res.data.body.personnelTransferRecords.map(function(list,index){\r\n              lists.push({\r\n                times:list.createDate,\r\n                oldunit:list.office.name,\r\n                oldposition:list.originalPosition,\r\n                newunit:list.newOffice.name,\r\n                newposition:list.newOriginalPosition,\r\n                handles:list.createBy.name,\r\n                num:index+1 > 9?index+1:'0'+(index+1) ,\r\n                name: list.personnel.name,\r\n                sex:list.personnel.sex==0?'男':'女',\r\n                age:list.personnel.age,\r\n                id:list.personnel.id,\r\n                input:\"\",\r\n              })\r\n            })\r\n            that.tableData = lists\r\n          }else{\r\n            this.$message.error(res.data.msg); \r\n          }\r\n        })\r\n      }\r\n    },\r\n    //显示树状图\r\n    treelistbtn(){\r\n      this.treelist = true\r\n      this.searchlist = false\r\n    },\r\n    //表格行点击事件\r\n    handleCurrentChange(val){\r\n      this.currentRow = val;\r\n      this.peoplemsg = {\r\n        times:val.times,\r\n        oldunit:val.oldunit,\r\n        oldposition:val.oldposition,\r\n        newunit:val.newunit,\r\n        newposition:val.newposition,\r\n        handles:val.handles,\r\n        num:val.num,\r\n        name: val.name,\r\n        sex:val.sex,\r\n        age:val.oldunit,\r\n        id:val.id,\r\n        input:\"\",\r\n      }\r\n    },\r\n    //导出\r\n    educebtn(){\r\n      let objs = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n        ids:''\r\n      } \r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: this.globals.requesturl + 'web/personneltransfer/export',\r\n        data: objs,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.errorCode == \"-1\"){\r\n          let url = res.data.body.path\r\n          let link = document.createElement('a')\r\n          link.style.display = 'none'\r\n          link.href = url\r\n          link.setAttribute('download', '人员调动记录表')\r\n          document.body.appendChild(link)\r\n          link.click()\r\n          this.$message.success(res.data.msg); \r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n      })\r\n    },\r\n  }\r\n}"]}]}