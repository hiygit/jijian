t\">\r\n          <el-breadcrumb-item :to=\"{ path: '/StandBook' }\">线索台账</el-breadcrumb-item>\r\n          <el-breadcrumb-item>更新进度</el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n      </div>\r\n    </div>\r\n    <div class=\"content home\">\r\n      <div class=\"StandBookEnter\">\r\n        <span class=\"border row1\"></span>\r\n        <span class=\"border row2\"></span>\r\n        <span class=\"border col1\"></span>\r\n        <span class=\"border col2\"></span>\r\n        <div class=\"StandBookEnterMain\" style=\"display: flex\">\r\n          <!-- 左侧 进程列表 -->\r\n          <div class=\"progressListLeftBox\">\r\n            <div class=\"progressListLeftTop\">\r\n              <div class=\"progressListbox\">\r\n                <img src=\"../../assets/schedule.png\" alt />\r\n                <span>进程列表</span>\r\n              </div>\r\n              <div class=\"topStepText\">\r\n                <p class=\"texto steptexto\" @click=\"leftget(0)\">\r\n                  <img src=\"../../assets/steponesel.png\" v-if=\"step == 0\" />\r\n                  <img src=\"../../assets/stepone.png\" v-else />\r\n                  <span :class=\"step == 0 ? 'stepselspan':''\">收件</span>\r\n                </p>\r\n                <p class=\"texto\" :class=\"step >= 1 ?'steptexto':''\" @click=\"leftget(1)\">\r\n                  <img src=\"../../assets/steptwosel.png\" v-if=\"step == 1\" />\r\n                  <img src=\"../../assets/steptwo.png\" v-else />\r\n                  <span :class=\"step == 1 ? 'stepselspan':''\">每周召开线索研判会</span>\r\n                </p>\r\n                <p class=\"texto\" :class=\"step >= 2 ?'steptexto':''\" @click=\"leftget(2)\">\r\n                  <img src=\"../../assets/stepthreesel.png\" v-if=\"step == 2\" />\r\n                  <img src=\"../../assets/stepthree.png\" v-else />\r\n                  <span :class=\"step == 2 ? 'stepselspan':''\">问题线索处置</span>\r\n                </p>\r\n                <p class=\"texto\" @click=\"leftget(3)\">\r\n                  <img src=\"../../assets/stepfoursel.png\" v-if=\"step == 3\" />\r\n                  <img src=\"../../assets/stepfour.png\" v-else />\r\n                  <span :class=\"step == 3 ? 'stepselspan':''\">处置结果落实</span>\r\n                </p>\r\n                <p class=\"texto lastTexto\" @click=\"leftget(4)\">\r\n                  <img src=\"../../assets/stepfivesel.png\" v-if=\"step == 4\" />\r\n                  <img src=\"../../assets/stepfive.png\" v-else />\r\n                  <span :class=\"step == 4 ? 'stepselspan':''\">归档</span>\r\n                </p>\r\n              </div>\r\n            </div>\r\n            <!-- 上传附件记录 -->\r\n            <div class=\"progressListLeftbottom\">\r\n              <div class=\"progressListbox\">\r\n                <img src=\"../../assets/accessory.png\" alt />\r\n                <span>上传附件记录</span>\r\n              </div>\r\n              <div class=\"accessoryBottom\">\r\n                <div\r\n                  class=\"accessoryBottom-text\"\r\n                  v-for=\"(item,index) in fileRecordList\"\r\n                  :key=\"index\"\r\n                >\r\n                  <p>\r\n                    <span>{{item.number}}{{(index+1 + '、')}}</span>\r\n                    {{item.file}}\r\n                  </p>\r\n                  <p style=\"text-align: right;color: #CBD9E8\">{{item.time}}</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 右侧 详细进度 -->\r\n          <div class=\"progressListRightBox\">\r\n            <div class=\"detailProgressTopBox\">\r\n              <p>\r\n                <img src=\"../../assets/amend.png\" alt />\r\n                <span>详细进度</span>\r\n              </p>\r\n              <!-- <button class=\"bottomBtnStyle\" @click=\"saverecord\">上传附件</button> -->\r\n              <div class=\"filesbtns\">\r\n                <el-button size=\"small\" type=\"primary\">上传附件</el-button>\r\n                <input\r\n                  type=\"file\"\r\n                  value=\"上传附件\"\r\n                  class=\"filesbtns-input\"\r\n                  multiple\r\n                  accept\r\n                  @click=\"headgetfile($event)\"\r\n                  title=\" \"\r\n                />\r\n              </div>\r\n              <!-- <div class=\"\">\r\n\r\n              </div>-->\r\n            </div>\r\n            <!-- 详细进度下的tab切换 -->\r\n            <div class=\"detailTab\">\r\n              <!-- 切换按钮 -->\r\n              <div class=\"tabButton\" v-show=\"isShow\">\r\n                <div class=\"tabButtonOne\" @click=\"get(1)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img v-if=\"pages==1\" src=\"../../assets/img.png\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 1 ? 'news':''\">谈话</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(2)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==2\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 2 ? 'news':''\">函询</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(3)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==3\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 3 ? 'news':''\">谈话+函询</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(4)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==4\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 4 ? 'news':''\">初核</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(5)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==5\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 5 ? 'news':''\">立案</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(6)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==6\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 6 ? 'news':''\">暂存</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(7)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==7\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 7 ? 'news':''\">了结</span>\r\n                </div>\r\n                <div class=\"tabButtonOne\" @click=\"get(8)\">\r\n                  <div class=\"imgBoxx\">\r\n                    <img src=\"../../assets/img.png\" v-if=\"pages==8\" alt />\r\n                    <img src=\"../../assets/img1.png\" v-else alt />\r\n                  </div>\r\n                  <span :class=\"pages == 8 ? 'news':''\">转办</span>\r\n                </div>\r\n              </div>\r\n\r\n              <!-- 切换内容 -->\r\n              <div class=\"contentTabBox\">\r\n                <textarea v-model=\"textareaobj.textarealist[pages-1].content\" class=\"textareaContent\" cols=\"30\" rows=\"10\" \r\n                placeholder=\"请输入\" :disabled=\"isdisable\"\r\n     