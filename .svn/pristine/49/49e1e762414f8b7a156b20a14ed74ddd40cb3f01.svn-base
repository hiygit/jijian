D,GAA7B;AACD;AACF,SA1BD;AA2BD;AACF,KAzNK;AA0NN;AACA+C,IAAAA,WA3NM,yBA2NO;AACX,WAAK9F,QAAL,GAAgB,IAAhB;AACA,WAAKD,UAAL,GAAkB,KAAlB;AACD,KA9NK;AA+NN;AACAgG,IAAAA,OAhOM,qBAgOG;AAAC,WAAKC,OAAL,CAAapD,IAAb,CAAkB;AAACqD,QAAAA,IAAI,EAAC,UAAN;AAAiBC,QAAAA,KAAK,EAAC;AAACC,UAAAA,OAAO,EAAC;AAAT;AAAvB,OAAlB;AAAuD,KAhO3D;AAiON;AACAC,IAAAA,eAlOM,2BAkOU9B,GAlOV,EAkOc;AAClB,UAAIhD,EAAE,GAAGgD,GAAG,CAAChD,EAAb;AACA,WAAK0E,OAAL,CAAapD,IAAb,CAAkB;AAACqD,QAAAA,IAAI,EAAC,UAAN;AAAiBC,QAAAA,KAAK,EAAC;AAAC5E,UAAAA,EAAE,EAACA,EAAJ;AAAO6E,UAAAA,OAAO,EAAC;AAAf;AAAvB,OAAlB;AACD,KArOK;AAsON;AACAE,IAAAA,MAvOM,oBAuOE;AACN,WAAKL,OAAL,CAAaM,EAAb,CAAgB,CAAC,CAAjB;AACD;AAzOK;AAvEK,CAAf","sourcesContent":["import qs from 'qs'\r\nexport default {\r\n  data(){\r\n    return{\r\n      searchlist:true,//查询结果\r\n      treelist:false,//树状图显示\r\n      personnelpops:false,//调动弹框显示切换\r\n      //搜索查询\r\n      formInline:{\r\n        user:\"\",\r\n        region:\"\"\r\n      },\r\n      //表格数据\r\n      tableData: [],\r\n      //级联选择\r\n      unitslist:[],\r\n      unitvalue:\"\",\r\n      //树状图\r\n      datas: [],\r\n      //树状图结构\r\n      defaultProps: {\r\n        children: 'children',\r\n        label: 'label',\r\n        value:\"value\",\r\n        dex:\"\"\r\n      },\r\n      sexlist:[],//性别数组\r\n      //人员信息\r\n      peoplemsg:{\r\n        name:\"\",\r\n        age:\"\",\r\n        sex:\"\",\r\n        units:\"\",\r\n        duty:\"\",\r\n        input:\"\",\r\n        id:\"\"\r\n      },\r\n    }\r\n  },\r\n  created(){\r\n    document.title = \"人员调动\"\r\n    let that = this;\r\n    //性别\r\n    this.$ajax({\r\n      method: \"POST\",\r\n      url: this.globals.requesturl + \"web/dict\",\r\n      data: { type: \"sex\" },\r\n      headers: { \"Content-Type\": \"application/x-www-form-urlencoded\" },\r\n      transformRequest: [\r\n        function(data) {\r\n          return qs.stringify(data);\r\n        }\r\n      ]\r\n    }).then(res => {\r\n      if (res.data.success && res.data.errorCode == \"-1\") {