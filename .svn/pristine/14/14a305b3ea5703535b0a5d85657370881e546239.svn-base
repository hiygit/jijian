E,GAAG,CAAC5E,GAPO;AAQfC,QAAAA,IAAI,EAAE2E,GAAG,CAAC3E,IARK;AASfC,QAAAA,GAAG,EAAC0E,GAAG,CAAC1E,GATO;AAUfC,QAAAA,GAAG,EAACyE,GAAG,CAACjF,OAVO;AAWfS,QAAAA,EAAE,EAACwE,GAAG,CAACxE,EAXQ;AAYf2C,QAAAA,KAAK,EAAC;AAZS,OAAjB;AAcD,KAtOK;AAuON;AACA8B,IAAAA,QAxOM,sBAwOI;AAAA;;AACR,UAAIpE,IAAI,GAAG;AACTC,QAAAA,MAAM,EAAEC,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,OAAb,CAAqB,UAArB,CAAX,EAA6CC,GAD5C;AAET+D,QAAAA,GAAG,EAAC;AAFK,OAAX;AAIA,WAAKzD,KAAL,CAAW;AACTC,QAAAA,MAAM,EAAE,MADC;AAETC,QAAAA,GAAG,EAAE,KAAKC,OAAL,CAAaC,UAAb,GAA0B,8BAFtB;AAGTjD,QAAAA,IAAI,EAAEiC,IAHG;AAITiB,QAAAA,OAAO,EAAE;AAAC,0BAAgB;AAAjB,SAJA;AAKTC,QAAAA,gBAAgB,EAAE,CAAC,UAAUnD,IAAV,EAAgB;AACjC,iBAAOD,EAAE,CAACqD,SAAH,CAAapD,IAAb,CAAP;AACD,SAFiB;AALT,OAAX,EAQGqD,IARH,CAQQ,UAAAC,GAAG,EAAE;AACX,YAAGA,GAAG,CAACtD,IAAJ,CAASuD,SAAT,IAAsB,IAAzB,EAA8B;AAC5B,cAAIR,GAAG,GAAGO,GAAG,CAACtD,IAAJ,CAASyD,IAAT,CAAc8C,IAAxB;AACA,cAAIC,IAAI,GAAG1E,QAAQ,CAAC2E,aAAT,CAAuB,GAAvB,CAAX;AACAD,UAAAA,IAAI,CAACE,KAAL,CAAWC,OAAX,GAAqB,MAArB;AACAH,UAAAA,IAAI,CAACI,IAAL,GAAY7D,GAAZ;AACAyD,UAAAA,IAAI,CAACK,YAAL,CAAkB,UAAlB,EAA8B,SAA9B;AACA/E,UAAAA,QAAQ,CAAC2B,IAAT,CAAcqD,WAAd,CAA0BN,IAA1B;AACAA,UAAAA,IAAI,CAACO,KAAL;;AACA,UAAA,MAAI,CAACvC,QAAL,CAAca,OAAd,CAAsB/B,GAAG,CAACtD,IAAJ,CAAS0E,GAA/B;AACD,SATD,MASK;AACH,UAAA,MAAI,CAACF,QAAL,CAAcC,KAAd,CAAoBnB,GAAG,CAACtD,IAAJ,CAAS0E,GAA7B;AACD;AACF,OArBD;AAsBD,KAnQK;AAoQN;AACAsC,IAAAA,MArQM,oBAqQE;AACN,WAAKC,OAAL,CAAaC,EAAb,CAAgB,CAAC,CAAjB;AACD;AAvQK;AAvDK,CAAf","sourcesContent":["import qs from 'qs'\r\nexport default {\r\n  data(){\r\n    return{\r\n      searchlist:true,//查询结果\r\n      treelist:false,//树状图显示\r\n      personnelpops:false,//调动弹框显示切换\r\n      //搜索查询\r\n      formInline:{\r\n        user:\"\",\r\n        region:\"\"\r\n      },\r\n      currentRow: 2,//选中行数\r\n      //表格数据\r\n      tableData: [],\r\n      //级联选择\r\n      unitslist:[],\r\n      unitvalue:\"\",\r\n      value:\"\",\r\n      //树状图\r\n      datas: [],\r\n      //树状图结构\r\n      defaultProps: {\r\n        children: 'children',\r\n        label: 'label',\r\n        value:\"value\",\r\n        dex:\"\"\r\n      },\r\n      //人员信息\r\n      peoplemsg:{\r\n        times:'',\r\n        oldunit:'',\r\n        oldposition:'',\r\n        newunit:'',\r\n        newposition:'',\r\n        handles:'',\r\n        num:'',\r\n        name: '',\r\n        sex:'',\r\n        age:'',\r\n        id:''\r\n      },\r\n    }\r\n  },\r\n  created(){\r\n    document.title = \"人员调动记录\"\r\n  },\r\n  mounted(){\r\n    let objs = {\r\n      userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n      name:\"\"\r\n    } \r\n    this.init(objs);\r\n    this.initunits();\r\n    this.treelistarr();\r\n  },\r\n  methods:{\r\n    // 初始化\r\n    init(objs){\r\n      let that = this; \r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: that.globals.requesturl + 'web/personneltransfer/recordList',\r\n        data: objs,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.errorCode == \"-1\"){\r\n          let lists = [];\r\n          if(res.data.body.personnelTransferRecords.length != 0){\r\n            res.data.body.personnelTransferRecords.map(function(list,index){\r\n              lists.push({\r\n                times:list.createDate,\r\n                oldunit:list.office.name,\r\n                oldposition:list.originalPosition,\r\n                newunit:list.newOffice.name,\r\n                newposition:list.newOriginalPosition,\r\n                handles:list.createBy.name,\r\n                num:index+1 > 9?index+1:'0'+(index+1) ,\r\n                name: list.personnel.name,\r\n                sex:list.personnel.sex==0?'男':'女',\r\n                age:list.personnel.age,\r\n                id:list.personnel.id,\r\n                input:\"\",\r\n              })\r\n            })\r\n          }\r\n          that.tableData = lists\r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n      })\r\n    },\r\n    // 查询单位\r\n    initunits(){\r\n      let that = this; \r\n      let objs = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n      } \r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: that.globals.requesturl + 'web/office/list',\r\n        data: objs,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.errorCode == \"-1\"){\r\n          let lists = [];\r\n          res.data.body.officeList.map(function(list,index){\r\n            lists.push({\r\n              label:list.name,\r\n              value:list.id,\r\n              dex:index,\r\n              children: list.hasChildren==false?\"\":[{}],\r\n              ishasChildren:list.hasChildren==false?false:true\r\n            })\r\n          })\r\n          that.unitslist = lists\r\n          that.datas = lists\r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n      })\r\n    },\r\n    //级联事件\r\n    unitChanges(data) {\r\n      let that = this;\r\n      let objs = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n        parentId:data[0]\r\n      } \r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: that.globals.requesturl + 'web/office/getChildren',\r\n        data: objs,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.errorCode == \"-1\"){\r\n          let lists = [];\r\n          res.data.body.officeList.map(function(list,index){\r\n            lists.push({\r\n              label:list.name,\r\n              value:list.id,\r\n              dex:index,\r\n              children: list.hasChildren==false?\"\":[{}],\r\n              ishasChildren:list.hasChildren==false?false:true\r\n            })\r\n          })\r\n          let nums = 0;\r\n          that.unitslist.map(function(list,index){\r\n            if(list.value == data){\r\n              nums = index\r\n            }\r\n          })\r\n          that.unitslist[nums].children = lists\r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n      })\r\n    },\r\n    //确定调动\r\n    submitchangeunit(){\r\n      let that = this;\r\n      let querststr = {\r\n        personnelId:that.peoplemsg.id,\r\n        newOfficeId:that.unitvalue[that.unitvalue.length-1],\r\n        newOriginalPosition:that.peoplemsg.input,\r\n        userId:JSON.parse(localStorage.getItem('userinfo')).isd,\r\n      };\r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: that.globals.requesturl + 'web/personneltransfer/transfer',\r\n        data: querststr,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.success || res.data.errorCode == -1){\r\n          that.personnelpops = false\r\n          this.$message.success('调动成功'); \r\n        }else{\r\n          this.$message.error('调动失败'); \r\n        }\r\n      })\r\n    },\r\n    //搜素提交按钮\r\n    onSubmit(){\r\n      let searchstr = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n        name:this.formInline.region\r\n      }\r\n      this.init(searchstr)\r\n      this.treelist = false\r\n      this.searchlist = true\r\n    },\r\n    // 树状图\r\n    treelistarr(){\r\n      let that = this; \r\n      let objs = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n      } \r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: that.globals.requesturl + 'web/office/findAllOffices',\r\n        data: objs,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.errorCode == \"-1\"){\r\n          that.treedata = res.data.body.data\r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n      })\r\n    },\r\n    //树状图选择\r\n    handleNodeClick(data,node,resolve) {\r\n      let that = this; \r\n      if(node.childNodes.length == 0){\r\n        that.treelist = false\r\n        that.searchlist = true\r\n        let objs = {\r\n          userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n          officeId:node.id\r\n        } \r\n        this.$ajax({\r\n          method: \"POST\",\r\n          url: that.globals.requesturl + 'web/personnel/index',\r\n          data: objs,\r\n          headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n          transformRequest: [function (data) { \r\n            return qs.stringify(data)\r\n          }]\r\n        }).then(res=>{\r\n          if(res.data.errorCode == \"-1\"){\r\n            let lists = [];\r\n            res.data.body.personnels.map(function(list,index){\r\n              lists.push({\r\n                num:index+1 > 9?index+1:'0'+(index+1) ,\r\n                name: list.name,\r\n                sex:list.sex==0?'男':'女',\r\n                age:list.age,\r\n                unit:list.office.name,\r\n                position:list.presentOccupation,\r\n                id:list.id\r\n              })\r\n            })\r\n            that.tableData = lists\r\n          }else{\r\n            this.$message.error(res.data.msg); \r\n          }\r\n        })\r\n      }\r\n    },\r\n    //显示树状图\r\n    treelistbtn(){\r\n      this.treelist = true\r\n      this.searchlist = false\r\n    },\r\n    //表格行点击事件\r\n    handleCurrentChange(val){\r\n      this.currentRow = val;\r\n      this.peoplemsg = {\r\n        times:val.times,\r\n        oldunit:val.oldunit,\r\n        oldposition:val.oldposition,\r\n        newunit:val.newunit,\r\n        newposition:val.newposition,\r\n        handles:val.handles,\r\n        num:val.num,\r\n        name: val.name,\r\n        sex:val.sex,\r\n        age:val.oldunit,\r\n        id:val.id,\r\n        input:\"\",\r\n      }\r\n    },\r\n    //导出\r\n    educebtn(){\r\n      let objs = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n        ids:''\r\n      } \r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: this.globals.requesturl + 'web/personneltransfer/export',\r\n        data: objs,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.errorCode == \"-1\"){\r\n          let url = res.data.body.path\r\n          let link = document.createElement('a')\r\n          link.style.display = 'none'\r\n          link.href = url\r\n          link.setAttribute('download', '人员调动记录表')\r\n          document.body.appendChild(link)\r\n          link.click()\r\n          this.$message.success(res.data.msg); \r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n      })\r\n    },\r\n    // 返回上一页\r\n    goback(){\r\n      this.$router.go(-1)\r\n    }\r\n  }\r\n}"]}]}