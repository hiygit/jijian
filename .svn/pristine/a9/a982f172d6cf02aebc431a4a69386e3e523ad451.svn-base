{"remainingRequest":"D:\\我的项目\\纪检委\\byqc\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\我的项目\\纪检委\\byqc\\src\\views\\UserCenter\\UserManage.vue?vue&type=script&lang=js&","dependencies":[{"path":"D:\\我的项目\\纪检委\\byqc\\src\\views\\UserCenter\\UserManage.vue","mtime":1576667814359},{"path":"D:\\我的项目\\纪检委\\byqc\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\我的项目\\纪检委\\byqc\\node_modules\\thread-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\我的项目\\纪检委\\byqc\\node_modules\\babel-loader\\lib\\index.js","mtime":499162500000},{"path":"D:\\我的项目\\纪检委\\byqc\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\我的项目\\纪检委\\byqc\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":["//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n\r\nimport qs from 'qs'\r\nexport default {\r\n  name: \"UserManage\",\r\n  data() {\r\n    return {\r\n      loading:false,\r\n      ifadd:false,\r\n      centerDialogVisible:false,//是否确认删除\r\n      tableHeight: \"66vh\",\r\n      readonly: true,\r\n      personnelpops: false, //弹框\r\n      isif:[],\r\n      checkList: [],\r\n      numm:\"\",\r\n      updata:{\r\n        loginName:\"\",//登录名\r\n        name:\"\",//姓名\r\n        password:\"\",//密码\r\n        phone:\"\",//手机\r\n        loginFlag:\"\",//是否允许登陆\r\n      },\r\n      value: '',\r\n         \r\n      tableData: [],\r\n      page:0,//第几页\r\n      pagesize:20,//每页显示条数\r\n      delval:\"\",//删除下标\r\n    };\r\n  },\r\n  created() {\r\n    document.title = \"用户管理\"\r\n    let that = this\r\n    this.$ajax({\r\n      method: \"POST\",\r\n      url: this.globals.requesturl + 'web/dict',\r\n      data: {type:'yes_no'},\r\n      headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n      transformRequest: [function (data) { \r\n        return qs.stringify(data)\r\n      }]\r\n    }).then(res=>{\r\n      if(res.data.success && res.data.errorCode == '-1'){\r\n        res.data.body.dictValueList.map(function(list){\r\n          that.isif.push({value:list.value,label:list.label})\r\n        })\r\n      }else{\r\n        this.$message.error(res.data.msg); \r\n      }\r\n    })\r\n  },\r\n  mounted() {\r\n    let objs = {\r\n      userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n      pageNo:this.page,\r\n      pageSize:this.pagesize,\r\n    } \r\n    this.init(objs);\r\n  },\r\n  methods: {\r\n    //刷新\r\n    refresh(){\r\n      let that = this\r\n      that.loading  =true\r\n      setTimeout(function(){\r\n        that.loading  =false\r\n      },1500)\r\n      let objs = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n        pageNo:this.page,\r\n        pageSize:this.pagesize,\r\n      } \r\n      this.init(objs);\r\n    },\r\n    //取消\r\n    quit(){\r\n      this.ifadd=false\r\n      this.personnelpops = false\r\n      this.updata = {\r\n        loginName:\"\",//登录名\r\n        name:\"\",//姓名\r\n        password:\"\",//密码\r\n        phone:\"\",//手机\r\n        loginFlag:\"\",//是否允许登陆\r\n      }\r\n    },\r\n    //添加按钮\r\n    addbtn(){\r\n      this.ifadd = true\r\n      this.personnelpops = true\r\n    },\r\n    //保存\r\n    savebtnyes(){\r\n      let that = this\r\n      let objs = that.updata\r\n      objs.userId = JSON.parse(localStorage.getItem('userinfo')).isd\r\n      if(objs.loginName == \"\"){\r\n        this.$message.error(\"请填写登录名\"); \r\n      }else if(objs.name == \"\"){\r\n        this.$message.error(\"请填写姓名\"); \r\n      }else if(objs.phone==\"\"){\r\n        this.$message.error(\"请填写手机号\"); \r\n      }else if(!(/^1[3456789]\\d{9}$/.test(objs.phone))){\r\n        this.$message.error(\"手机号格式有误\"); \r\n      }else{\r\n        this.$ajax({\r\n          method: \"POST\",\r\n          url: this.globals.requesturl + 'web/user/save',\r\n          data: objs,\r\n          headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n          transformRequest: [function (data) { \r\n            return qs.stringify(data)\r\n          }]\r\n        }).then(res=>{\r\n          if(res.data.success && res.data.errorCode == '-1'){\r\n            that.personnelpops = false\r\n            if(that.ifadd){\r\n              objs.loginFlag = (objs.loginFlag==that.isif[0].value?that.isif[0].label:that.isif[1].label)\r\n              that.tableData.push(objs)\r\n            }else{\r\n              // this.tableData[that.delval] = objs\r\n              this.$set(this.tableData,that.delval,objs)\r\n              this.updata = {\r\n                loginName:\"\",//登录名\r\n                name:\"\",//姓名\r\n                password:\"\",//密码\r\n                phone:\"\",//手机\r\n                loginFlag:\"\",//是否允许登陆\r\n              }\r\n            }\r\n            this.$message.success(res.data.msg); \r\n            this.ifadd=false\r\n          }else{\r\n            this.$message.error(res.data.msg); \r\n          }\r\n        })\r\n      }\r\n    },\r\n    //修改\r\n    updataitem(row){\r\n      let that = this\r\n      this.delval = row\r\n      let objs = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n        id:that.tableData[row].id\r\n      } \r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: this.globals.requesturl + 'web/user/form',\r\n        data: objs,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.success && res.data.errorCode == '-1'){\r\n          that.personnelpops = true\r\n          that.updata = {\r\n            loginName:res.data.body.loginName,//登录名\r\n            name:res.data.body.name,//姓名\r\n            password:res.data.body.password,//密码\r\n            phone:res.data.body.phone,//手机\r\n            loginFlag:res.data.body.loginFlag,//是否允许登陆\r\n            id:res.data.body.id,//是否允许登陆\r\n            password:res.data.body.password,\r\n          }\r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n      })\r\n    },\r\n    // 删除\r\n    romoveitem(row){\r\n      this.delval = row\r\n      this.centerDialogVisible = true\r\n    },\r\n    subremove(){\r\n      let that = this,id = that.tableData[that.delval].id;\r\n      let objs = {\r\n        userId: JSON.parse(localStorage.getItem('userinfo')).isd,\r\n        id:id,\r\n      } \r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: that.globals.requesturl + 'web/user/delete',\r\n        data: objs,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.success && res.data.errorCode == -1){\r\n          this.$message.success(res.data.msg); \r\n          that.tableData.splice(that.delval,1)\r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n        that.centerDialogVisible = false\r\n      })\r\n    },\r\n    // 初始化\r\n    init(objs){\r\n      let that = this\r\n      this.$ajax({\r\n        method: \"POST\",\r\n        url: that.globals.requesturl + 'web/user/list',\r\n        data: objs,\r\n        headers: {'Content-Type': 'application/x-www-form-urlencoded'},\r\n        transformRequest: [function (data) { \r\n          return qs.stringify(data)\r\n        }]\r\n      }).then(res=>{\r\n        if(res.data.success && res.data.errorCode == -1){\r\n          let listarr = [];\r\n          res.data.body.userList.map(function(list){\r\n            listarr.push({\r\n              loginName:list.loginName,//登录名\r\n              name:list.name,//姓名\r\n              phone:list.phone,//手机号\r\n              loginFlag:list.loginFlag==that.isif[0].value?that.isif[0].label:that.isif[1].label,\r\n              id:list.id\r\n            })\r\n          })\r\n          that.tableData = listarr\r\n        }else{\r\n          this.$message.error(res.data.msg); \r\n        }\r\n      })\r\n    },\r\n    //下标处理\r\n    indexMethod(index){\r\n      return index+1>9?index+1:'0'+(index+1)\r\n    },\r\n    // 表格样式的处理\r\n    cellStyle({ row, column, rowIndex, columnIndex }) {\r\n      //基数偶数延时不同\r\n      if (rowIndex % 2 != 0) {\r\n        return {\r\n          \"background-color\": \"#2162A8\",\r\n          \"text-align\": \"center\",\r\n          color: \"#fff\",\r\n          \"font-weight\": \"normal\",\r\n          padding: \"5px 0\",\r\n          height: \"60px\"\r\n        };\r\n      } else {\r\n        return {\r\n          \"background-color\": \"#02468F\",\r\n          \"text-align\": \"center\",\r\n          color: \"#fff\",\r\n          \"font-weight\": \"normal\",\r\n          padding: \"5px 0\",\r\n          height: \"60px\"\r\n        };\r\n      }\r\n    },\r\n    headerCellStyle({ row, rowIndex }) {\r\n      return {\r\n        \"background-color\": \"#027DBC\",\r\n        \"text-align\": \"center\",\r\n        color: \"#fff\",\r\n        \"font-weight\": \"normal\",\r\n        \"font-size\": \"16px\"\r\n      };\r\n    }\r\n  },\r\n  watch: {}\r\n};\r\n",null]}