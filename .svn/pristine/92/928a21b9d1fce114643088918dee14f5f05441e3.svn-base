\r\n      <div class=\"navcrumbedlist\">\r\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n          <el-breadcrumb-item :to=\"{ path: '/Home' }\">首页</el-breadcrumb-item>\r\n          <el-breadcrumb-item><span style=\"color:#03d0ff\">个人信息</span></el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n      </div>\r\n    </div>\r\n    <div class=\"content home\">\r\n      <div class=\"AddInfo\">\r\n        <span class=\"border row1\"></span>\r\n        <span class=\"border row2\"></span>\r\n        <span class=\"border col1\"></span>\r\n        <span class=\"border col2\"></span>\r\n        <div class=\"AddInfoMain\">\r\n          <div class=\"addInfoMainTop\">\r\n            <div class=\"AddInfoMainLeft\">\r\n              <el-form :model=\"form\" :label-width=\"labelWidth\" :rules=\"rules\" ref=\"form\">\r\n                <el-form-item label=\"上传一寸红底照片\" prop=\"redBottomPhoto\">\r\n                  <div class=\"avatar-uploader\">\r\n                    <img v-if=\"form.redBottomPhoto\" :src=\"form.redBottomPhoto\" />\r\n                    <img v-else src=\"../../assets/addpic.png\" alt srcset />\r\n                    <input\r\n                      type=\"file\"\r\n                      accept=\".jpg, .jpeg, .png, .gif, .JPG, .JPEG\"\r\n                      @change=\"handleAvatarSuccess($event)\"\r\n                    />\r\n                  </div>\r\n                </el-form-item>\r\n                <el-form-item label=\"姓名\" prop=\"name\">\r\n                  <el-input :disabled=\"disabled\" v-model=\"form.name\" placeholder=\"请填写姓名\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"性别\" prop=\"sex\">\r\n                  <el-radio\r\n                    :disabled=\"disabled\"\r\n                    v-model=\"form.sex\"\r\n                    :label=\"item.value\"\r\n                    v-for=\"(item,index) in sexlist\"\r\n                    :key=\"index\"\r\n                  >{{item.label}}</el-radio>\r\n                </el-form-item>\r\n                <el-form-item label=\"籍贯\" prop=\"nativePlace\">\r\n                  <el-input :disabled=\"disabled\" v-model=\"form.nativePlace\" placeholder=\"请填写籍贯\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"学历\" prop=\"educationBackground\">\r\n                  <el-select\r\n                    v-model=\"form.educationBackground\"\r\n                    placeholder=\"请选择学历\"\r\n                    :disabled=\"disabled\"\r\n                  >\r\n                    <el-option\r\n                      :label=\"item.label\"\r\n                      :value=\"item.value\"\r\n                      v-for=\"(item,index) in degreelist\"\r\n                      :key=\"index\"\r\n                    ></el-option>\r\n                  </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"民族\" prop=\"nation\">\r\n                  <el-select v-model=\"form.nation\" placeholder=\"请选择名族\" :disabled=\"disabled\">\r\n                    <el-option\r\n                      :label=\"item\"\r\n                      :value=\"item\"\r\n                      v-for=\"(item,index) in nationalities\"\r\n                      :key=\"index\"\r\n                    ></el-option>\r\n                  </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"婚姻状况\" prop=\"maritalStatus\">\r\n                  <el-select\r\n                    v-model=\"form.maritalStatus\"\r\n                    placeholder=\"请选择婚姻状况\"\r\n                    :disabled=\"disabled\"\r\n                  >\r\n                    <el-option\r\n                      :label=\"item.label\"\r\n                      :value=\"item.value\"\r\n                      v-for=\"item in marriagelist\"\r\n                      :key=\"item.value\"\r\n                    ></el-option>\r\n                  </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"出生日期\" prop=\"birthday\">\r\n                  <el-date-picker\r\n                    :disabled=\"disabled\"\r\n                    v-model=\"form.birthday\"\r\n                    type=\"date\"\r\n                    placeholder=\"请选择出生日期\"\r\n                    value-format=\"yyyy-MM-dd\"\r\n                  ></el-date-picker>\r\n                </el-form-item>\r\n                <el-form-item label=\"身份证号\" prop=\"idCard\">\r\n                  <el-input\r\n                    :disabled=\"disabled\"\r\n                    v-model=\"form.idCard\"\r\n                    oninput=\"if(value.length>18)value=value.slice(0,18)\"\r\n                    onkeyup=\"value=value.replace(/[^\\d|X]/g,'')\"\r\n                    placeholder=\"请填写身份证号\"\r\n                  ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"政治面貌\" prop=\"politicsStatus\">\r\n                  <el-select\r\n                    v-model=\"form.politicsStatus\"\r\n                    placeholder=\"请选择政治面貌\"\r\n                    :disabled=\"disabled\"\r\n                  >\r\n                    <el-option\r\n                      :label=\"item.label\"\r\n                      :value=\"item.value\"\r\n                      v-for=\"(item,index) in politicallist\"\r\n                      :key=\"index\"\r\n                    ></el-option>\r\n                  </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"入党时间\" prop=\"partyTime\">\r\n                  <el-date-picker\r\n                    :disabled=\"disabled\"\r\n                    v-model=\"form.partyTime\"\r\n                    type=\"date\"\r\n                    placeholder=\"请选择入党时间\"\r\n                    value-format=\"yyyy-MM-dd\"\r\n                  ></el-date-picker>\r\n                </el-form-item>\r\n                <el-form-item label=\"参加工作时间\" prop=\"workTime\">\r\n                  <el-date-picker\r\n                    :disabled=\"disabled\"\r\n                    v-model=\"form.workTime\"\r\n                    type=\"date\"\r\n                    placeholder=\"请选择参加工作时间\"\r\n                    value-format=\"yyyy-MM-dd\"\r\n                  ></el-date-picker>\r\n                </el-form-item>\r\n                <el-form-item label=\"工作单位\" prop=\"companyName\">\r\n                   <el-input :disabled=\"disabled\" v-model=\"form.companyName\" readonly v-if=\"showinput\" ></el-input>\r\n                      <el-cascader v-model=\"form.companyName\" :options=\"unitslist\" @active-item-change=\"unitChanges\" placeholder=\"请选择单位名称\" :props=\"{value: 'id',label: 'name',children: 'children'}\">\r\n                  </el-cascader>\r\n                </el-form-item>\r\n                <el-form-item label=\"现任职务\" prop=\"presentOccupation\">\r\n                  <el-input :disabled=\"disabled\" v-model=\"form.presentOccupation\" placeholder=\"请填写现任职务\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"职务级别\" prop=\"jobLevel\">\r\n                  <el-select v-model=\"form.jobLevel\" placeholder=\"请选择职务级别\" :disabled=\"disabled\">\r\n                    <el-option :label=\"item.label\"\r\n                      :value=\"item.value\" v-for=\"(item,index) in dutyRankList\" :key=\"index\"></el-option>\r\n                  </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"现任职务时间\" prop=\"incumbentTime\">\r\n                  <el-date-picker\r\n                    :disabled=\"disabled\"\r\n                    v-model=\"form.incumbentTime\"\r\n                    type=\"date\"\r\n                    placeholder=\"请选择现任职务时间\"\r\n                    value-format=\"yyyy-MM-dd\"\r\n                  ></el-date-picker>\r\n                </el-form-item>\r\n                <el-form-item label=\"人员类型\" prop=\"personnelcategory\">\r\n                  <el-select\r\n                    v-model=\"form.personnelcategory\"\r\n                    placeholder=\"请选择人员类型\"\r\n                    :disabled=\"disabled\"\r\n                  >\r\n                    <el-option\r\n                      :label=\"item.label\"\r\n                      :value=\"item.value\"\r\n                      v-for=\"(item,index) in Personneltype\"\r\n                      :key=\"index\"\r\n                    ></el-option>\r\n                  </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"职称\" prop=\"titleJobs\">\r\n                  <el-input :disabled=\"disabled\" v-model=\"form.titleJobs\" placeholder=\"请填写职称\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"职称级别\" prop=\"titleJobsLevel\">\r\n                  <el-input\r\n                    :disabled=\"disabled\"\r\n                    v-model=\"form.titleJobsLevel\"\r\n                    placeholder=\"请填写职称级别\"\r\n                  ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"是否政协委员\" prop=\"isCPPCCMemberOrNot\">\r\n                  <el-radio\r\n                    :disabled=\"disabled\"\r\n                    v-model=\"form.isCPPCCMemberOrNot\"\r\n                    :label=\"item.value\"\r\n                    v-for=\"(item,index) in isif\"\r\n                    :key=\"index\"\r\n                  >{{item.label}}</el-radio>\r\n                </el-form-item>\r\n                <el-form-item label=\"是否人大代表\" prop=\"isNPCDEputiesOrNot\">\r\n                  <el-radio\r\n                    :disabled=\"disabled\"\r\n                    v-model=\"form.isNPCDEputiesOrNot\"\r\n                    :label=\"item.value\"\r\n                    v-for=\"(item,index) in isif\"\r\n                    :key=\"index\"\r\n                  >{{item.label}}</el-radio>\r\n                </el-form-item>\r\n                <el-form-item label=\"是否党代表\" prop=\"isPartyRepresentativeOrNot\">\r\n                  <el-radio\r\n                    :disabled=\"disabled\"\r\n                    v-model=\"form.isPartyRepresentativeOrNot\"\r\n                    :label=\"item.value\"\r\n                    v-for=\"(item,index) in isif\"\r\n                    :key=\"index\"\r\n                  >{{item.label}}</el-radio>\r\n                </el-form-item>\r\n                <el-form-item label=\"现住址\" prop=\"presentAddress\">\r\n                  <el-input\r\n                    :disabled=\"disabled\"\r\n                    v-model=\"form.presentAddress\"\r\n                    placeholder=\"请填写详细地址\"\r\n                  ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"联系电话\" prop=\"phone\">\r\n                  <el-input\r\n                    :disabled=\"disabled\"\r\n                    v-model=\"form.phone\"\r\n                    oninput=\"if(value.length>11)value=value.slice(0,11)\"\r\n                    onkeyup=\"this.value=this.value.replace(/\\D/g,'')\"\r\n                    placeholder=\"请输入联系电话\"\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </el-form>\r\n            </div>\r\n            <div class=\"AddInfoMainRight\">\r\n              <div class=\"rightBlock\">\r\n                <div class=\"rightTableFlex\">\r\n                  <div class=\"rightTableTitle\">\r\n                    <p class=\"rightTableMainTitle\">家庭主要成员及主要社会关系</p>\r\n                    <p class=\"rightTableSubTitle\">需填写（配偶、子女、双方父母、本人兄弟姐妹）若无该身份人员。则无需填写。注:已故上述人员，也需填写。</p>\r\n                  </div>\r\n                  <div class=\"rightTableBtn\">\r\n                    <button class=\"addTableRow\" @click=\"addfamily\">添加</button>\r\n                  </div>\r\n                </div>\r\n                <div>\r\n                  <el-table\r\n                    :data=\"familyMembers\"\r\n                    border\r\n                    :height=\"tableHeight\"\r\n                    stripe\r\n                    highlight-current-row\r\n                    style=\"background-color:transparent;width:100%;box-sizing:border-box; \"\r\n                    :cell-style=\"cellStyle\"\r\n                    :header-cell-style=\"headerCellStyle\"\r\n                  >\r\n                    <el-table-column label=\"称谓\" prop=\"appellation\">\r\n                      <template scope=\"scope\">\r\n                        <el-input\r\n                          :disabled=\"disabled\"\r\n                          style=\"display:inline-block;width:90%;margin:0 auto;\"\r\n                          size=\"small\"\r\n                          v-model=\"scope.row.appellation\"\r\n                          placeholder=\"请输入内容\"\r\n                        />\r\n                      </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"姓名\" prop=\"name\">\r\n                      <template scope=\"scope\">\r\n                        <el-input\r\n                          :disabled=\"disabled\"\r\n                          style=\"display:inline-block;width:90%;margin:0 auto;\"\r\n                          size=\"small\"\r\n                          v-model=\"scope.row.name\"\r\n                          placeholder=\"请输入内容\"\r\n                        />\r\n                      </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"政治面貌\" prop=\"politicsStatus\">\r\n                      <template scope=\"scope\">\r\n                        <el-select\r\n                          v-model=\"scope.row.politicsStatus\"\r\n                          placeholder=\"请选择政治面貌\"\r\n                          :disabled=\"disabled\"\r\n                        >\r\n                          <el-option\r\n                            :label=\"item.label\"\r\n                            :value=\"item.value\"\r\n                            v-for=\"(item,index) in politicallist\"\r\n                            :key=\"index\"\r\n                          ></el-option>\r\n                        </el-select>\r\n                      </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"年龄\" prop=\"age\">\r\n                      <template scope=\"scope\">\r\n                        <el-input\r\n                          :disabled=\"disabled\"\r\n                          style=\"display:inline-block;width:90%;margin:0 auto;\"\r\n                          size=\"small\"\r\n                          v-model=\"scope.row.age\"\r\n                          placeholder=\"请输入内容\"\r\n                        />\r\n                      </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"职务(级别)\" prop=\"presentOccupation\">\r\n                      <template scope=\"scope\">\r\n                        <el-input\r\n                          :disabled=\"disabled\"\r\n                          style=\"display:inline-block;width:90%;margin:0 auto;\"\r\n                          size=\"small\"\r\n                          v-model=\"scope.row.presentOccupation\"\r\n                          placeholder=\"请输入内容\"\r\n                        />\r\n                      </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"工作单位\" prop=\"companyName\">\r\n                      <template scope=\"scope\">\r\n                        <el-cascader\r\n                          v-model=\"scope.row.companyName\"\r\n                          :options=\"unitslist\"\r\n                          @expand-change=\"unitChanges\"\r\n                          placeholder=\"请选择单位名称\"\r\n                        ></el-cascader>\r\n                      </template>\r\n                    </el-table-column>\r\n                  </el-table>\r\n                </div>\r\n              </div>\r\n              <div class=\"rightBlock\">\r\n                <div class=\"rightTableFlex\">\r\n                  <div class=\"rightTableTitle\">\r\n                    <p class=\"rightTableMainTitle\" style=\"margin-top:10px;\">个人及配偶、共同生活的子女拥有的房产情况</p>\r\n                  </div>\r\n                  <div class=\"rightTableBtn\">\r\n                    <button class=\"addTableRow\" @click=\"housingbtn\">添加</button>\r\n                  </div>\r\n                </div>\r\n                <div>\r\n                  <el-table\r\n                    :data=\"housingSituations\"\r\n                    border\r\n                    :height=\"tableHeight\"\r\n                    stripe\r\n                    :disabled=\"disabled\"\r\n                    style=\"background-color:transparent;width:100%;box-sizing:border-box;\"\r\n                    :cell-style=\"cellStyle\"\r\n                    :header-cell-style=\"headerCellStyle\"\r\n                  >\r\n                    <el-table-column\r\n                      label=\"序号\"\r\n                      type=\"index\"\r\n                      :index=\"indexinfo\"\r\n                      width=\"100\"\r\n                      prop=\"serialNumber\"\r\n                    >\r\n                      <!-- <template scope=\"scope\">\r\n                        <el-input :disabled=\"disabled\" style=\"display:inline-block;width:90%;margin:0 auto;\" size=\"small\" v-model=\"scope.row.serialNumber\" placeholder=\"请输入内容\"/>\r\n                      </template>-->\r\n                    </el-table-column>\r\n                    <el-table-column label=\"房号\" prop=\"roomNumber\">\r\n                      <template scope=\"scope\">\r\n                        <el-input\r\n                          style=\"display:inline-block;width:90%;margin:0 auto;\"\r\n                          size=\"small\"\r\n                          v-model=\"scope.row.roomNumber\"\r\n                          :disabled=\"disabled\"\r\n                          placeholder=\"请输入内容\"\r\n                        />\r\n                      </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"建筑面积(m²)\" prop=\"coveredArea\">\r\n                      <template scope=\"scope\">\r\n                        <el-input\r\n                          style=\"display:inline-block;width:90%;margin:0 auto;\"\r\n                          size=\"small\"\r\n                          v-model=\"scope.row.coveredArea\"\r\n                          :disabled=\"disabled\"\r\n                          placeholder=\"请输入内容\"\r\n                        />\r\n                      </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"购买时间\" prop=\"purchasingDate\">\r\n                      <template scope=\"scope\">\r\n                        <el-date-picker\r\n                          :disabled=\"disabled\"\r\n                          v-model=\"scope.row.purchasingDate\"\r\n                          type=\"date\"\r\n                          placeholder=\"请选择购买时间\"\r\n                          value-format=\"yyyy-MM-dd\"\r\n                        ></el-date-picker>\r\n                      </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"购入价(万元)\" prop=\"purchasePrice\">\r\n                      <template scope=\"scope\">\r\n                        <el-input\r\n                          :disabled=\"disabled\"\r\n                          style=\"display:inline-block;width:90%;margin:0 auto;\"\r\n                          size=\"small\"\r\n                          v-model=\"scope.row.purchasePrice\"\r\n                          placeholder=\"请输入内容\"\r\n                        />\r\n                      </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"产权人\" prop=\"propertyOwner\">\r\n                      <template scope=\"scope\">\r\n                        <el-input\r\n                          :disabled=\"disabled\"\r\n                          style=\"display:inline-block;width:90%;margin:0 auto;\"\r\n                          size=\"small\"\r\n                          v-model=\"scope.row.propertyOwner\"\r\n                          placeholder=\"请输入内容\"\r\n                        />\r\n                      </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"所在位置\" prop=\"location\">\r\n                      <template scope=\"scope\">\r\n                        <el-input\r\n                          :disabled=\"disabled\"\r\n                          style=\"display:inline-block;width:90%;margin:0 auto;\"\r\n                          size=\"small\"\r\n                          v-model=\"scope.row.location\"\r\n                          placeholder=\"请输入内容\"\r\n                        />\r\n                      </template>\r\n                    </el-table-column>\r\n                  </el-table>\r\n                </div>\r\n              </div>\r\n              <!-- textarea -->\r\n              <div class=\"rightTextareaBlock\">\r\n                <p class=\"rightTextareaTitle\">个人工作简历</p>\r\n                <el-input\r\n                  :disabled=\"disabled\"\r\n                  v-model=\"form.personalResume\"\r\n                  :autosize=\"{ minRows: 5, maxRows: 5}\"\r\n                  type=\"textarea\"\r\n                  placeholder=\"请填写工作经历\"\r\n                />\r\n              </div>\r\n              <div class=\"rightTextareaBlock\">\r\n                <p class=\"rightTextareaTitle\">奖惩情况</p>\r\n                <el-input\r\n                  :disabled=\"disabled\"\r\n                  v-model=\"form.rewardsAndPunishment\"\r\n                  :autosize=\"{ minRows: 5, maxRows: 5}\"\r\n                  type=\"textarea\"\r\n                  placeholder=\"请填写奖惩情况\"\r\n                />\r\n              </div>\r\n              <div class=\"rightTextareaBlock\" style=\"border-bottom:none;\">\r\n                <p class=\"rightTextareaTitle\">其他相关情况</p>\r\n   