l-table-column prop=\"clueNumber\" label=\"线索件数\"></el-table-column>\r\n        <el-table-column prop=\"disponsalWay\" label=\"进度\"></el-table-column>\r\n        <el-table-column prop=\"name\" label=\"被反映人姓名\"></el-table-column>\r\n        <el-table-column prop=\"office\" label=\"被反映人工作单位\"></el-table-column>\r\n        <el-table-column prop=\"presentOccupation\" label=\"被反映人职务\"></el-table-column>\r\n        <el-table-column label=\"操作\" width=\"180px\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"primary\" @click=\"handleEdit(scope.$index, scope.row)\">查看</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Navbar from \"@/components/Navbar/index.vue\";\r\nimport qs from \"qs\";\r\nexport default {\r\n  name: \"StandBook\",\r\n  data() {\r\n    return {\r\n      userinfo: {}, //用户信息缓存\r\n      totalList: [], //所有数据\r\n      tablearr: [], \r\n      lists: {},\r\n      sourcelist: [], //线索来源\r\n      progresslist: [], //进度\r\n      keyworktext: \"\", //关键字查找\r\n      tableHeadList: [\r\n        // {\r\n        //   label: \"序号\",\r\n        //   prop: \"number\"\r\n        // },\r\n        // {\r\n        //   label: \"收件时间\",\r\n        //   prop: \"createDate\"\r\n        // },\r\n        // {\r\n        //   label: \"线索来源\",\r\n        //   prop: \"clueStoTheSource\"\r\n        // },\r\n        // {\r\n        //   label: \"线索编号\",\r\n        //   prop: \"letterNumber\"\r\n        // },\r\n        // {\r\n        //   label: \"线索件数\",\r\n        //   prop: \"clueNumber\"\r\n        // },\r\n        // {\r\n        //   label: \"进度\",\r\n        //   prop: \"disponsalWay\"\r\n        // },\r\n        // {\r\n        //   label: \"被反映人姓名\",\r\n        //   prop: \"name\"\r\n        // },\r\n        // {\r\n        //   label: \"被反映人工作单位\",\r\n        //   prop: \"office\"\r\n        // },\r\n        // {\r\n        //   label: \"被反